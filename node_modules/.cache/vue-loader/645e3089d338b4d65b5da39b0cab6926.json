ext-center'}\r\n            ],\r\n            users:[],\r\n            errors:[],\r\n            ref:firebase.firestore().collection('users'),\r\n        }\r\n    },created(){\r\n        this.ref.onSnapshot((querySnapshot)=>{\r\n            this.users = [];\r\n            querySnapshot.forEach((doc) =>{\r\n                this.users.push({\r\n                    key:doc.id,\r\n                    name:doc.data().name\r\n                })\r\n            })\r\n        })\r\n    },\r\n    methods:{\r\n        details(user){\r\n            this.$router.push({name:'ShowUsers', params:{id:user.key}})\r\n        }\r\n    }\r\n}\r\n\r\n</script>"]}]}