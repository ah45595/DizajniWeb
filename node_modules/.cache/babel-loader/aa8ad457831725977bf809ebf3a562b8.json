A,UAAA,EAAA,EAAA,IAAA,CAAA;AAAA;AAAA,OAAA;AACA;AAHA;AAxBA,CAAA","sourcesContent":["<template>\r\n<b-row>\r\n<b-col cols=\"12\">\r\n    <h2>\r\n        User User\r\n        <b-link href=\"#/add-user\">Add User</b-link>\r\n    </h2>\r\n    <b-table striped hover :items=\"users\" :fields=\"fields\">\r\n        <template v-slot:cell(actions)=\"data\">\r\n            <b-button @click.stop=\"details(data.item)\" variant=\"primary\">Details</b-button>\r\n        </template>\r\n    </b-table>\r\n</b-col>\r\n</b-row>\r\n</template>\r\n<script>\r\nimport firebase from '../../Firebase'\r\nimport router from '../../routes'\r\n\r\nexport default{\r\n    /*eslint-disable */\r\n    name:'UsersList',\r\n    data(){\r\n        return{\r\n            fields:[\r\n                { key: 'name', label:'name', sortable:true, 'class':'text-left'},\r\n                { key: 'actions', label:'Actions', 'class':'text-center'}\r\n            ],\r\n            users:[],\r\n            errors:[],\r\n            ref:firebase.firestore().collection('users'),\r\n        }\r\n    },created(){\r\n        this.ref.onSnapshot((querySnapshot)=>{\r\n            this.users = [];\r\n            querySnapshot.forEach((doc) =>{\r\n                this.users.push({\r\n                    key:doc.id,\r\n                    name:doc.data().name\r\n                })\r\n            })\r\n        })\r\n    },\r\n    methods:{\r\n        details(user){\r\n            this.$router.push({name:'ShowUsers', params:{id:user.key}})\r\n        }\r\n    }\r\n}\r\n\r\n</script>"],"sourceRoot":"src/components/users"}]}