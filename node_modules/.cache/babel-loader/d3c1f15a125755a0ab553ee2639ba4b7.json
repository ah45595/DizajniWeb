,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA;AADA,SAAA;AAGA,OAJA,EAIA,KAJA,CAIA,UAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,2BAAA,EAAA,KAAA,CAAA;AACA,OANA;AAOA;AAfA;AApBA,CAAA","sourcesContent":["<template>\r\n  <b-row>\r\n    <b-col cols=\"12\">\r\n      <h2>\r\n        Edit user\r\n        <b-link href=\"#/users-list\">(user List)</b-link>\r\n      </h2>\r\n      <b-jumbotron>\r\n        <template slot=\"header\">\r\n          {{user.name}}\r\n        </template>\r\n        <template slot=\"lead\">\r\n          name: {{user.name}}<br>\r\n          Description: {{user.description}}<br>\r\n          email: {{user.email}}<br>\r\n        </template>\r\n        <hr class=\"my-4\">\r\n        <b-btn class=\"edit-btn\" variant=\"success\" @click.stop=\"edituser(key)\">Edit</b-btn>\r\n        <b-btn variant=\"danger\" @click.stop=\"deleteuser(key)\">Delete</b-btn>\r\n      </b-jumbotron>\r\n      \r\n    </b-col>\r\n    \r\n  </b-row>\r\n</template>\r\n\r\n<script>\r\n/*eslint-disable */\r\n\r\nimport firebase from '../../Firebase'\r\nimport router from '../../routes'\r\n\r\nexport default {\r\n  name: 'Showuser',\r\n  data () {\r\n    return {\r\n      key: '',\r\n      user: {}\r\n    }\r\n  },\r\n//created perdoret per me ekzekutu automatikisht kodin ose queryn ne momentin qe komponenta bohet render!\r\ncreated () {\r\n    const ref = firebase.firestore().collection('users').doc(this.$route.params.id);\r\n    ref.get().then((doc) => {\r\n      if (doc.exists) {\r\n        this.key = doc.id;\r\n        this.user = doc.data();\r\n      } else {\r\n        alert(\"No such document!\");\r\n      }\r\n    });\r\n  },\r\n  methods: {\r\n    edituser (id) {\r\n      this.$router.push({\r\n        name: 'EditUser',\r\n        params: { id: id }\r\n      })\r\n    },\r\n    deleteuser (id) {\r\n      firebase.firestore().collection('users').doc(id).delete().then(() => {\r\n        this.$router.push({\r\n          name: 'UsersList'\r\n        })\r\n      }).catch((error) => {\r\n        alert(\"Error removing document: \", error);\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  .jumbotron {\r\n    padding: 2rem;\r\n  }\r\n  .edit-btn {\r\n    margin-right: 20px;\r\n    width: 70px;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components/users"}]}