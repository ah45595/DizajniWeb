m-group id=\"nameGroup\"\r\n                    horizontal\r\n                    :label-cols=\"4\"\r\n                    breakpoint=\"md\"\r\n                    label=\"Enter name\">\r\n            <b-form-input id=\"name\" v-model.trim=\"user.name\"></b-form-input>\r\n          </b-form-group>\r\n          <b-form-group id=\"descGroup\"\r\n                    horizontal\r\n                    :label-cols=\"4\"\r\n                    breakpoint=\"md\"\r\n                    label=\"Enter Description\">\r\n              <b-form-textarea id=\"description\"\r\n                         v-model=\"user.description\"\r\n                         placeholder=\"Enter something\"\r\n                         :rows=\"2\"\r\n                         :max-rows=\"6\">{{user.description}}</b-form-textarea>\r\n          </b-form-group>\r\n          <b-form-group id=\"emailGroup\"\r\n                    horizontal\r\n                    :label-cols=\"4\"\r\n                    breakpoint=\"md\"\r\n                    label=\"Enter email\">\r\n            <b-form-input id=\"email\" v-model.trim=\"user.email\"></b-form-input>\r\n          </b-form-group>\r\n          <b-button type=\"submit\" variant=\"primary\">Save</b-button>\r\n        </b-form>\r\n      </b-jumbotron>\r\n    </b-col>\r\n  </b-row>\r\n</template>\r\n\r\n<script>\r\n/*eslint-disable */\r\n\r\nimport firebase from '../../Firebase'\r\nimport router from '../../routes'\r\n\r\nexport default {\r\n  name: 'AddUser',\r\n  data () {\r\n    return {\r\n      ref: firebase.firestore().collection('users'),\r\n      user: {}\r\n    }\r\n  },\r\n  methods: {\r\n    onSubmit (evt) {\r\n      evt.preventDefault()\r\n\r\n      this.ref.add(this.user).then(() => {\r\n        this.user.name = ''\r\n        this.user.description = ''\r\n        this.user.email = ''\r\n        this.$router.push({\r\n          name: 'UsersList'\r\n        })\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n  .jumbotron {\r\n    padding: 2rem;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components/users"}]}